
<div class="container">
<div class="alert alert-info">
<p id="notice"><%= notice %></p>
</div>

<h1>Listing My Shorten Urls</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Original Url</th>
      <th>Shorten Url</th>
      
    </tr>
  </thead>

  <tbody>
    <% @shortenr.each do |shorten| %>
      <tr>
        <td><%= shorten.original_url %></td>
        <td><%= "http://localhost:3000/#{shorten.tiny_url}" %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
